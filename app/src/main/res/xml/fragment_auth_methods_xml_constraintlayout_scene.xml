<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:motionInterpolator="easeInOut"
        motion:staggered="0.65">
        <KeyFrameSet>

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/ivAppIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0"
            android:scaleX="0.5"
            android:scaleY="0.5"
            motion:layout_constraintBottom_toTopOf="@+id/textView2"
            motion:layout_constraintDimensionRatio="1:1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_chainStyle="packed"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0.4"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnGmail"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/ivAppIcon"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnGmail"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0.4"
            android:scaleX="0.5"

            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnFacebook"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnFacebook"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0.4"
            android:scaleX="0.5"
            android:scaleY="0.5"

            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnSignUp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/btnGmail"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnSignUp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0.4"
            android:scaleX="0.5"
            android:scaleY="0.5"

            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnLogin"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/btnFacebook"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnLogin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0.4"
            android:scaleX="0.5"
            android:scaleY="0.5"

            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/btnSignUp"
            motion:transitionEasing="standard" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="1"
            android:scaleX="1"
            android:scaleY="1"

            android:translationX="0dp"
            android:translationY="0dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnGmail"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/ivAppIcon"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnGmail"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="1"
            android:scaleX="1"
            android:scaleY="1"
            android:translationX="0dp"
            android:translationY="0dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnFacebook"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnFacebook"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="1"
            android:scaleX="1"
            android:scaleY="1"
            android:translationX="0dp"
            android:translationY="0dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnSignUp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/btnGmail"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnSignUp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="1"
            android:scaleX="1"
            android:scaleY="1"
            android:translationX="0dp"
            android:translationY="0dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnLogin"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/btnFacebook"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnLogin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="1"
            android:scaleX="1"
            android:scaleY="1"
            android:translationX="0dp"
            android:translationY="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/btnSignUp"
            motion:transitionEasing="standard" />
    </ConstraintSet>
</MotionScene>