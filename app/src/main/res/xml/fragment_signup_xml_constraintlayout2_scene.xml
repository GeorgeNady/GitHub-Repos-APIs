<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1400"
        motion:motionInterpolator="easeInOut"
        motion:staggered="0.6">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_extra_large"
            android:layout_marginEnd="@dimen/mg_extra_large"
            android:alpha="0"
            android:scaleX="1.5"
            android:scaleY="1.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/etEmailLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_chainStyle="packed"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/etEmailLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/etPasswordLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView2"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/etPasswordLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnSignin"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/etEmailLayout"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/etRePasswordLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnSignUp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/etPasswordLayout"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnSignUp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/linearLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/etPasswordLayout"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/linearLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:alpha="0"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toTopOf="@+id/btnSignUp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/btnSignin"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/btnSignin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mg_large"
            android:layout_marginTop="@dimen/mg_default"
            android:layout_marginEnd="@dimen/mg_large"
            android:layout_marginBottom="@dimen/mg_default"
            android:alpha="0"
            android:scaleX="0.5"
            android:scaleY="0.5"
            android:translationX="-100dp"
            android:translationY="-50dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/linearLayout"
            motion:transitionEasing="standard" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>